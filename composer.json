{
  "name": "leadvertex/plugin-macros-excel",
  "description": "Plugin for export orders from leadvertex.com in Excel (xlsx, xls, csv) format",
  "type": "project",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Timur Kasumov aka XAKEPEHOK",
      "email": "support@leadvertex.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Leadvertex\\Plugin\\Instance\\Macros\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../plugin-core",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-json": "*",
    "box/spout": "^3.1",
    "xakepehok/array-graphql": "^0.0.1",
    "leadvertex/plugin-core-macros": "^0.13.0",
    "leadvertex/plugin-component-translations": "^0.1.1"
  }
}
